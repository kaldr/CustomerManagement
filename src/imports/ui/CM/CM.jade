md-toolbar.navi(flex='noshrink')
  .md-toolbar-tools.navi-tools(layout="row" layout-align='center center')
      md-button.navi-tools-button(ng-repeat='(sref,navi) in cm.navigation' ui-sref='{{sref}}' name='{{sref}}') 
        i.fa(ng-class='navi.iconClass' hide-gt-xs)
        span.navi-txt {{navi.name}}
        
#mainView(flex ui-view='CM' layout-wrap layout='column' layout-fill flex-order-xs="-1")
